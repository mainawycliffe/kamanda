{
  "name": "kamanda",
  "version": "0.4.1-5",
  "description": "Kamanda - A Firebase CLI Companion Tool",
  "main": "index.js",
  "scripts": {
    "test": "go test ./... -v",
    "postinstall": "node ./bin/index.js install",
    "preuninstall": "node ./bin/index.js uninstall",
    "prepublish": "node_modules/babel-cli/bin/babel.js index.js --out-dir bin --presets es2015"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mainawycliffe/kamanda.git"
  },
  "keywords": [
    "Firebase",
    "Kamanda",
    "Firebase CLI",
    "Firebase Auth",
    "Authentication",
    "Firestore"
  ],
  "preferGlobal": true,
  "author": "Maina Wycliffe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mainawycliffe/kamanda/issues"
  },
  "homepage": "https://github.com/mainawycliffe/kamanda#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "tar": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "goBinary": {
    "name": "kamanda",
    "path": "./bin",
    "url": "https://github.com/mainawycliffe/kamanda/releases/download/v{{version}}/kamanda_{{version}}_{{platform}}_{{arch}}.tar.gz"
  }
}
